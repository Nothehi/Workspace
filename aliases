alias npm='docker run --rm --network=host -v "$PWD":/app -w /app workspace-node npm'
alias psql='docker run -it --rm --network=host workspace-postgres psql'
alias pg_dump='docker run -it --rm --network=host workspace-postgres pg_dump'
alias pg_restore='docker run -it --rm --network=host workspace-postgres pg_restore'
alias composer='docker run --rm --network=host -v "$PWD":/app -w /app workspace-php composer'
alias php='docker run --rm -it --network=host -v "$PWD":/app -w /app workspace-php php'